{
  "name": "electron-demo",
  "productName": "electron-demo",
  "description": "Dev Forum Electron Demo",
  "version": "0.0.1",
  "license": "UNLICENCED",
  "main": "./main.js",
  "devDependencies": {
    "electron": "^3.0.9",
    "electron-builder": "^20.38.5"
  },
  "scripts": {
    "build": "electron-builder --windows --publish always"
  },
  "build": {
    "appId": "electron-demo",
    "files": [
    ],
    "extraResources": [
      {
        "from": "./resources",
        "to": ".",
        "filter": [
          "*.*"
        ]
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/milliman.ico"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowElevation": "true"
    },
    "directories": {
      "output": "dist/installer/"
    }
  }
}
